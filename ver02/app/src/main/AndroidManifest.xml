<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.andpjt.catchfood">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_catchfood"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_catchfood_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-5632859719839583~6620488051"/>

        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.andpjt.catchfood.activity.MainActivity" />

        <activity android:name="com.andpjt.catchfood.activity.InitActivity"
            android:parentActivityName="com.andpjt.catchfood.activity.MainActivity" />
        <activity
            android:name="com.andpjt.catchfood.activity.SettingActivity"
            android:parentActivityName="com.andpjt.catchfood.activity.MainActivity" />
    </application>

</manifest>